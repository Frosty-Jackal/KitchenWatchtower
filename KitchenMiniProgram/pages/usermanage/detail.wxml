<view class="container container-gray">
	<view class="group">
		<view class="group-header">基本信息</view>
		<view class="group-body">
			<view class="input-list">
				<view class="input-item">
					<text class="input-item-label">用户名</text>
					<view class="input-item-content">
						<input type="text"  value="{{object_id}}" bindinput="inputname"></input> 
					</view>
				</view>
        <view class="input-item">
					<text class="input-item-label">用户电话</text>
					<view class="input-item-content">
						<input type="text"  value="{{title}}" ></input>
					</view>
				</view>
        <view class="input-item">
					<text class="input-item-label">身份证号</text>
					<view class="input-item-content">
						<input type="text"  value="{{content}}"></input>
					</view>
				</view>
        <view class="input-item">
					<text class="input-item-label">创建时间</text>
					<view class="input-item-content">
						<input type="text"  value="{{create_time}}" ></input>
					</view>
				</view>
      </view>
    </view>
  </view>
</view>